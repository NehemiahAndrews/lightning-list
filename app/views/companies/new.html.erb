<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add a new company</h1>
  <%= simple_form_for @company do |f| %>
    <%= f.input :name %>
    <%= f.input :phone_number %>
    <%= f.input :contact_name %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
  <% end %>
</div>